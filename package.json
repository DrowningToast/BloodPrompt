{
	"name": "bloodprompt",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"prettier": " prettier --plugin-search-dir --write \"./**/*.{js,json,md,html,css,scss,svelte}\"",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"postinstall": "pnpm db:push",
		"studio": "pnpx prisma studio",
		"db:push": "prisma db push",
		"db:force-push": "prisma db push --accept-data-loss",
		"db:generate": "prisma generate",
		"db:force-reset": "prisma db push --force-reset",
		"db:pull": "prisma db pull"
	},
