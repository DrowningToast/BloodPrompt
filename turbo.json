{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env.*local"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "pg:push": {
      "cache": false
    },
    "pg:force-push": {
      "cache": false
    },
    "pg:force-reset": {
      "cache": false
    },
    "pg:generate": {
      "cache": false
    },
    "db:studio": {
      "cache": true
    },
    "db:seed": {
      "cache": false
    }
  }
}